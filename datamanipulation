import pandas as pd
data = pd.read_csv("/content/01.Data Cleaning and Preprocessing.csv")
type(data)
pd.core.frame.DataFrame
print(data)
print(data.info)
print(data.describe())
data=data.drop_duplicates()
print(data)
print(data.isnull())
print(data.isnull().sum())
print(data.notnull())
data.isnull().sum().sum()
data2=data.fillna(value=0)
print(data2)
data3=data.fillna(method='pad')
print(data3)
data4=data.fillna(method='bfill')
print(data4)
